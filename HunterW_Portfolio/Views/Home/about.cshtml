@{ 
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">

<head>

<meta charset="utf-8">
<meta http-equiv="X-UA-Compatible" content="IE=edge">
<meta name="viewport" content="width=device-width, initial-scale=1">
<meta name="description" content="Minimal Portfolio Theme">
<meta name="keywords" content="responsive, retina ready, html5, css3, creative, portfolio, bootstrap theme" />
<meta name="author" content="KingStudio.ro">

<!-- favicon -->
<link rel="icon" href="~/Images/newfavicon.png">
<!-- page title -->
<title>Hunter Williams</title>
<!-- bootstrap css -->
<link href="~/Content/bootstrap.min.css" rel="stylesheet">
<!-- css -->
<link href="~/Content/style.css" rel="stylesheet">
<link href="~/Content/animate.css" rel="stylesheet">
<!-- fonts -->
<link href="https://fonts.googleapis.com/css?family=Raleway:400,700,800,900" rel="stylesheet">
<link href="~/Content/font-awesome.min.css" rel="stylesheet" type="text/css">
<link href='~/fonts/FontAwesome.otf' rel='stylesheet' type='text/css'>
<link rel="stylesheet" href="~/Content/linear-icons.css">

<!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
<!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
<!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
    <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
<![endif]-->

</head>

<body>

<!-- preloader -->
<div id="preloader">
    <div class="spinner spinner-round"></div>
</div>
<!-- / preloader -->

<!-- header -->
<header>
    <!-- nav -->
    <nav class="navbar navbar-default">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target=".navbar-collapse">
                <span class="sr-only">Toggle navigation</span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                </button>
                <a class="navbar-brand" href="@Url.Action("Index", "Home")"><img src="~/Images/newlogo.png" alt="logo"></a>
            </div><!-- / navbar-header -->
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    <li>
                        <a href="@Url.Action("Index", "Home")"><span>HOME</span></a>
                    </li>
                    <li class="active">
                        <a href="@Url.Action("About", "Home")"><span>ABOUT</span></a>
                    </li>
                    <li>
                        <a href="@Url.Action("Portfolio", "Home")"<span>PORTFOLIO</span></a>
                    </li>
                    <li>
                        <a href="@Url.Action("Codingchallenges", "Home")"><span>CODING CHALLENGES</span></a>
                    </li>
                    <li>
                        <a href="@Url.Action("Contact", "Home")"><span>CONTACT</span></a>
                    </li>
                </ul>
            </div><!--/ nav-collapse -->
        </div><!-- / container -->
    </nav>
    <!-- / nav -->

    <!-- header-banner -->
    <div id="header-banner">
        <div class="banner-content single-page text-center">
            <div class="banner-info">
                <h1>ABOUT HUNTER</h1>
                <p>Who is this guy?</p>
            </div><!-- / banner-info -->
        </div><!-- / banner-content -->
    </div>
    <!-- / header-banner -->
</header>
<!-- / header -->

<!-- content -->

<div id="page-content" class="container">
    <div class="section-w-image">
        <div class="row">
            <div class="col-sm-6 about-me-img"><p></p></div>
            <div class="col-sm-6 col-sm-offset-6 about-description">
                <h4>D. HUNTER WILLIAMS</h4>
				<p>Having grown up in High Point, North Carolina, I am currently learning web development at Coder Foundry in nearby Kernersville.
				Having had an interest in computers from a very young age, I thoroughly enjoy building applications. Formerly a photographer, I have a keen eye for aesthetics and detail.
				As such, his favorite computer language is CSS - Cascading Style Sheets.</p>
                <p>When not working, I spend time exploring caves, photographing landscapes, and downhill skiing.</p>
                <br />
            
                <h4 class="space-top">SKILLS</h4>
                <p><strong>JavaScript</strong></p>
                <p><strong>CSS</strong></p>
                <p><strong>HTML</strong></p>
                <p><strong>C#</strong></p>
                <br />
                
                <h4 class="space-top">EDUCATION</h4>
                <p>Web Development: <strong>Coder Foundry</strong> July - September 2019</p>
                <p>Photography: <strong>Randolph Community College</strong> August 2012 - July 2014</p>
            </div><!-- / about-description -->
        </div><!-- / row -->
    </div><!-- / section-w-image -->

    <!-- facts 4 col -->
    <section id="facts">
        <div class="row">

            <!-- fact --
            <div class="col-sm-3 col-sm-offset-3 text-center">
                <i class="fact-icon lnr lnr-camera"></i>
                <h3 class="timer" id="clients" data-to="1550" data-speed="1500">1550+</h3>
                <h5 class="fact-title">INSTAGRAM POSTS</h5>
            </div>
            <!-- / fact -->

            <!-- fact --
            <div class="col-sm-3 text-center">
                <i class="fact-icon lnr lnr-star"></i>
                <h3 class="timer" id="projects" data-to="1992" data-speed="2000">1992</h3>
                <h5 class="fact-title">BIRTH YEAR</h5>
            </div>
            <!-- / fact -->

            <!-- fact --
            <div class="col-sm-4 text-center">
                <i class="fact-icon lnr lnr-rocket"></i>
                <h3 class="timer" id="coffee" data-to="69.2" data-speed="2500">69.2</h3>
                <h5 class="fact-title">FASTEST SPEED ON SKIS</h5>
            </div>
            <!-- / fact -->
        </div><!-- / row -->
    </section>
    <!-- / facts 4 col -->
</div><!-- / container -->

<!-- / content -->

<!-- footer -->
<footer>
    <div class="container">
        <p class="footer-info">© 2019 HUNTER WILLIAMS
            <span class="social pull-right">
                <a href="https://twitter.com/DHunterW" target="_blank"><i class="fa fa-twitter"></i></a>
                <a href="https://www.instagram.com/wrathillumines/" target="_blank"><i class="fa fa-instagram"></i></a>
                <a href="https://www.linkedin.com/in/hunter-williams-66791644/" target="_blank"><i class="fa fa-linkedin"></i></a>
            </span>
        </p>
    </div><!-- / container -->
</footer>
<!-- / footer -->

<!-- javascript -->
<script src="~/Scripts/jquery.min.js"></script>
<script src="~/Scripts/bootstrap.min.js"></script>
<script src="~/Scripts/jquery.easing.min.js"></script>

<!-- facts counter -->
<script src="~/Scripts/jquery.countTo.js"></script>
 <script type="text/javascript">
      $('.timer').countTo({
        refreshInterval: 60,
        formatter: function (value, options) {
          return value.toFixed(options.decimals);
        },
      });
</script>
<!-- / facts counter -->

<!-- preloader -->
<script src="~/Scripts/preloader.js"></script>
<!-- / preloader -->

<!-- / javascript -->
</body>

</html>