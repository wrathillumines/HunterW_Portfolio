@{ 
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">

<head>

<meta charset="utf-8">
<meta http-equiv="X-UA-Compatible" content="IE=edge">
<meta name="viewport" content="width=device-width, initial-scale=1">
<meta name="description" content="Minimal Portfolio Theme">
<meta name="keywords" content="responsive, retina ready, html5, css3, creative, portfolio, bootstrap theme" />
<meta name="author" content="KingStudio.ro">

<!-- favicon -->
<link rel="icon" href="~/Images/newfavicon.png">
<!-- page title -->
<title>Hunter Williams</title>
<!-- bootstrap css -->
<link href="~/Content/bootstrap.min.css" rel="stylesheet">
<!-- css -->
<link href="~/Content/style.css" rel="stylesheet">
<link href="~/Content/animate.css" rel="stylesheet">
<link href="~/Content/magnific-popup.css" rel="stylesheet">
<!-- fonts -->
<link href="https://fonts.googleapis.com/css?family=Raleway:400,700,800,900" rel="stylesheet">
<link href="~/Content/font-awesome.min.css" rel="stylesheet" type="text/css">
<link href='~/fonts/FontAwesome.otf' rel='stylesheet' type='text/css'>
<link rel="stylesheet" href="~/Content/linear-icons.css">

<!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
<!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
<!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
    <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
<![endif]-->

</head>

<body>

<!-- preloader -->
<div id="preloader">
    <div class="spinner spinner-round"></div>
</div>
<!-- / preloader -->

<!-- header -->
<header>
    <!-- nav -->
    <nav class="navbar navbar-default">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target=".navbar-collapse">
                <span class="sr-only">Toggle navigation</span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                </button>
                <a class="navbar-brand" href="@Url.Action("Index", "Home")"><img src="~/Images/newlogo.png" alt="logo"></a>
            </div><!-- / navbar-header -->
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    <li>
                        <a href="@Url.Action("Index", "Home")"><span>HOME</span></a>
                    </li>
                    <li>
                        <a href="@Url.Action("About", "Home")"><span>ABOUT</span></a>
                    </li>
                    <li class="active">
                        <a href="@Url.Action("Portfolio", "Home")"><span>PORTFOLIO</span></a>
                    </li>
                    <li>
                        <a href="@Url.Action("Codingchallenges", "Home")"><span>CODING CHALLENGES</span></a>
                    </li>
                    <li>
                        <a href="@Url.Action("Contact", "Home")"><span>CONTACT</span></a>
                    </li>
                </ul>
            </div><!--/ nav-collapse -->
        </div><!-- / container -->
    </nav>
    <!-- / nav -->

    <!-- header-banner -->
    <div id="header-banner">
        <div class="banner-content single-page text-center">
            <div class="banner-info">
                <h1>PORTFOLIO</h1>
                <p>Some of My Work</p>
            </div><!-- / banner-info -->
        </div><!-- / banner-content -->
    </div>
    <!-- / header-banner -->
</header>
<!-- / header -->

<!-- content -->

<!-- portfolio section -->
<section id="portfolio" class="w-shadow">
    <div class="container">
        <!-- portfolio filter --
        <ul class="portfolio-filter list-inline text-center">
            <li><a href="#" data-group="all" class="active">ALL</a></li>
            <li><a href="#" data-group="graphic-design">GRAPHIC DESIGN</a></li>
            <li><a href="#" data-group="branding">BRANDING</a></li>
            <li><a href="#" data-group="logo-design">LOGO DESIGN</a></li>
            <li><a href="#" data-group="illustration">ILLUSTRATION</a></li>
        </ul><!--end portfolio filter -->
        <ul class="row portfolio list-unstyled lightbox" id="grid">
            <!-- blog -->
            <li class="col-md-4 project" data-groups='["illustration"]'>
                <div class="img-bg-color primary">
                    <a href="https://dhwblog.azurewebsites.net" target="_blank" class="project-link"></a>
                    <!-- / project-link -->
                    <img src="~/Images/blogportfolioimage.jpg" alt="">
                    <!-- / project-image -->

                    <!-- project-hover-tools -->
                    <div class="project-hover-tools">
                        <a href="https://dhwblog.azurewebsites.net" target="_blank" class="view-btn">
                            <i class="lnr lnr-eye"></i>
                        </a>
                        <a href="~/Images/blogportfolioimage.jpg" class="open-gallery">
                            <i class="lnr lnr-frame-expand"></i>
                        </a>
                    </div><!-- / project-hover-tools -->

                    <!-- project-details -->
					<div class="project-details">
						<div class="col-md-11">
							<h5 class="project-title">BLOG</h5>
							<h6 class="project-title">The blog site employs some C# and a bit of SQL database management.</h6>
						</div>
					</div><!-- / project-details -->
                </div><!-- / img-bg-color -->
            </li>
            <!-- / blog -->
        
            <!-- bug tracker -->
            <li class="col-md-4 project" data-groups='["branding"]'>
				<div class="img-bg-color primary">
					<a href="#" class="project-link"></a>
					<!-- / project-link -->
					<img src="~/Images/bugtracker.jpg" alt="">
					<!-- / project-image -->
					<!-- project-hover-tools -->
					<div class="project-hover-tools">
						<a href="#" class="view-btn">
							<i class="lnr lnr-eye"></i>
						</a>
						<a href="~/Images/bugtracker.jpg" class="open-gallery">
							<i class="lnr lnr-frame-expand"></i>
						</a>
					</div><!-- / project-hover-tools -->
					<!-- project-details -->
					<div class="project-details">
						<h5 class="project-title">BUG TRACKER</h5>
						<p class="skill">COMING SOON</p>
					</div><!-- / project-details -->
				</div><!-- / img-bg-color -->
            </li>
            <!-- / bug tracker -->

            <!-- financial portal -->
            <li class="col-md-4 project" data-groups='["logo-design"]'>
                <div class="img-bg-color primary">
                    <a href="#" class="project-link"></a>
                    <!-- / project-link -->
                    <img src="~/Images/financialportal.jpg" alt="">
                    <!-- / project-image -->

                    <!-- project-hover-tools -->
                    <div class="project-hover-tools">
                        <a href="#" class="view-btn">
                            <i class="lnr lnr-eye"></i>
                        </a>
                        <a href="~/Images/financialportal.jpg" class="open-gallery">
                            <i class="lnr lnr-frame-expand"></i>
                        </a>
                    </div><!-- / project-hover-tools -->

                    <!-- project-details -->
                    <div class="project-details">
                        <h5 class="project-title">FINANCIAL PORTAL</h5>
                        <p class="skill">COMING SOON</p>
                    </div><!-- / project-details -->
                </div><!-- / img-bg-color -->
            </li>
            <!-- / financial portal -->

            <!-- project --
            <li class="col-xs-6 col-md-4 project" data-groups='["illustration", "logo-design", "branding"]'>
                <div class="img-bg-color primary">
                    <a href="@Url.Action("Singleproject", "Home")" class="project-link"></a>
                    <!-- / project-link --
                    <img src="~/Images/project.jpg" alt="">
                    <!-- / project-image --

                    <!-- project-hover-tools --
                    <div class="project-hover-tools">
                        <a href="@Url.Action("Singleproject", "Home")" class="view-btn">
                            <i class="lnr lnr-eye"></i>
                        </a>
                        <a href="~/Images/project.jpg" class="open-gallery">
                            <i class="lnr lnr-frame-expand"></i>
                        </a>
                    </div><!-- / project-hover-tools --

                    <!-- project-details --
                    <div class="project-details">
                        <h5 class="project-title">PROJECT TITLE</h5>
                        <p class="skill">SKILLS</p>
                    </div><!-- / project-details --
                </div><!-- / img-bg-color --
            </li>
            <!-- / project -->

            <!-- sizer -->
            <li class="col-xs-6 col-md-4 shuffle_sizer"></li>      
        </ul> <!-- / projects -->
    </div><!-- / container -->

</section>
<!-- / portfolio section -->

<!-- / content -->

<!-- footer -->
<footer>
    <div class="container">
        <p class="footer-info">© 2019 HUNTER WILLIAMS
            <span class="social pull-right">
                <a href="https://twitter.com/DHunterW" target="_blank"><i class="fa fa-twitter"></i></a>
                <a href="https://www.instagram.com/wrathillumines/" target="_blank"><i class="fa fa-instagram"></i></a>
                <a href="https://www.linkedin.com/in/hunter-williams-66791644/"><i class="fa fa-linkedin"></i></a>
            </span>
        </p>
    </div><!-- / container -->
</footer>
<!-- / footer -->

<!-- javascript -->
<script src="~/Scripts/jquery.min.js"></script>
<script src="~/Scripts/bootstrap.min.js"></script>
<script src="~/Scripts/jquery.easing.min.js"></script>

<!-- portfolio -->
<script src="~/Scripts/custom.js"></script>
<script src="~/Scripts/jquery.shuffle.min.js"></script>
<!-- lightbox -->
<script src="~/Scripts/jquery.magnific-popup.min.js"></script>
<script type="text/javascript">
// This will create a single gallery from all elements that have class "lightbox"
$('.lightbox').each(function() {
    $(this).magnificPopup({
        delegate: 'a.open-gallery', // the selector for gallery item
        type: 'image',
        gallery: {
          enabled:true
        }
    });
});
</script>
<!-- / lightbox -->
<!-- / portfolio -->

<!-- preloader -->
<script src="~/Scripts/preloader.js"></script>
<!-- / preloader -->

<!-- / javascript -->
</body>

</html>